	"ps-xml": {
		"scope": "xml,ps1xml",
		"prefix": "ps-xml",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\" ?>",
			"<Configuration>",
			"\t<!-- 子元素 DefaultSettings -->",
			"\t<!-- 子元素 SelectionSets -->",
			"\t<!-- 子元素 Controls -->",
			"\t<ViewDefinitions>",
			"\t\t<!-- 子元素 View -->",
			"\t</ViewDefinitions>",
			"</Configuration>\n"
		],
		"description": "总框架"
	},
	"ps-DefaultSettings": {
		"scope": "xml,ps1xml",
		"prefix": "ps-DefaultSettings",
		"body": [
			"<DefaultSettings>",
			"\t<!-- <ShowError/> -->",
			"\t<!-- <DisplayError/> -->",
			"\t<!-- <PropertyCountForTable>Int</PropertyCountForTable> -->",
			"\t<!-- <WrapTable/> -->",
			"\t<EnumerableExpansions>",
			"\t\t<!-- 子元素 EnumerableExpansion -->",
			"\t</EnumerableExpansions>",
			"</DefaultSettings>"
		],
		"description": "默认设置框架"
	},
	"ps-EnumerableExpansion": {
